{
  "name": "improrpg",
  "version": "0.0.5",
  "description": "improRPG is a live IMPROvisied multiplayer text based Role Play Game adventure story notebook sideapp",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"no test specified JUST DO IT \" ",
    "lint": "lint-staged",
    "lintit": "eslint .",
    "prepare": "husky install",
    "start": "node src/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riskieee/improrpg.git"
  },
  "keywords": [
    "multiplayer",
    "live",
    "text",
    "role",
    "play",
    "game",
    "rpg",
    "adventure",
    "story",
    "notebook",
    "sideapp"
  ],
  "author": "",
  "license": "GNU",
  "bugs": {
    "url": "https://github.com/riskieee/improrpg/issues"
  },
  "homepage": "https://github.com/riskieee/improrpg#readme",
  "devDependencies": {
    "eslint": "^7.27.0",
    "babel-eslint": "^10.1.0",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/plugin-syntax-top-level-await": "^7.12.13",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-vue": "^7.9.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "qrcode-terminal": "^0.12.0"
  }
}
