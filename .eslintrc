/* https://eslint.org/docs/rules/ */
{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    // "eslint:recommended",
    // "plugin:vue/essential",
    "airbnb",
    "prettier"
    // "prettier/@typescript-eslint",
    // "prettier/babel",
    // "prettier/flowtype",
    // "prettier/react",
    // "prettier/standard",
    // "prettier/unicorn",
    // "prettier/vue"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["vue"],
  "rules": {
    // "no-unused-vars": "warn",
    "no-console": "off",
    "eqeqeq": "off",
    "class-methods-use-this": "off"
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}
