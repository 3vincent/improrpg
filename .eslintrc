{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    // "eslint:recommended",
    "plugin:vue/essential",
    "airbnb",
    "prettier"
    // "prettier/@typescript-eslint",
    // "prettier/babel",
    // "prettier/flowtype",
    // "prettier/react",
    // "prettier/standard",
    // "prettier/unicorn",
    // "prettier/vue"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["vue"],
  "rules": {
    // "no-unused-vars": "warn",
    "eqeqeq": "off",
    "class-methods-use-this": "off"
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}
